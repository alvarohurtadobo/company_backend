<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="Python FastCGI" 
      path="*" 
      verb="*" 
      modules="FastCgiModule" 
      scriptProcessor="C:\Python37\python.exe|C:\Python37\lib\site-packages\wfastcgi.py" 
      resourceType="Unspecified" 
      requireAccess="Script" />
    </handlers>
  </system.webServer>

  <appSettings>
    <add key="PYTHONPATH" value="C:\inetpub\wwwroot\wood_center_backend\" />
    <add key="WSGI_HANDLER" value="wood_center.wsgi.application" />
    <add key="DJANGO_SETTINGS_MODULE" value="wood_center.settings" />
  </appSettings>
</configuration>